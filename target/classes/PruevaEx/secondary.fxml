<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="648.0" prefWidth="869.0" spacing="20.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PruevaEx.SecondaryController">
    <children>
      <AnchorPane prefHeight="654.0" prefWidth="830.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="personaTable" layoutY="33.0" prefHeight="190.0" prefWidth="829.0">
              <columns>
                <TableColumn fx:id="columPolicia" prefWidth="75.0" text="Nombre" />
                <TableColumn fx:id="ApellCol" prefWidth="75.0" text="Apellidos" />
              </columns>
            </TableView>
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Switch to Primary View" />
            <Button fx:id="btnAgregar" layoutX="232.0" layoutY="538.0" mnemonicParsing="false" onAction="#agregarPersona" text="Agregar" />
            <TextField fx:id="txtNombre" layoutX="118.0" layoutY="304.0" prefHeight="25.0" prefWidth="202.0" />
         </children>
      </AnchorPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
